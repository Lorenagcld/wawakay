@model IEnumerable<loginwawaky.DataModel.Materias>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idMateria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreMateria)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idMateria)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreMateria)
        </td>
    </tr>
}

</table>
